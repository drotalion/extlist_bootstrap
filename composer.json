{
  "name": "drotalion/extlist_bootstrap",
  "type": "typo3-cms-extension",
  "description": "Bootstrap for Extlist",
  "keywords": [
    "TYPO3",
    "extension"
  ],
  "license": [
    "GPL-2.0+"
  ],
  "require": {
    "typo3/cms-core": ">=6.2.0,<8.0"
  },
  "require-dev": {

  },
  "autoload": {
    "psr-4": {
      "Drotalion\\Extlist_bootstrap\\": "Classes"
    },
    "classmap": [
      "Classes/"
    ]
  },
  "replace": {
    "extlist_bootstrap": "self.version",
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
